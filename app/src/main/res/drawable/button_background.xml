<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <shape >
        <!-- Define the shape as a rectangle -->
        <solid android:color="#f0e9d6" /> <!-- Specify the background color -->

        <!-- Set the corner radius for rounded edges -->
        <corners android:radius="8dp" /> <!-- Change the radius as needed -->

        <!-- Optional: Add stroke/border color and width -->
        <stroke android:width="1dp" android:color="#000000" />
    </shape>


</selector>